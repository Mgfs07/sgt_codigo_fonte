<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">

    <changeSet id="202204290954-1" author="leonardo.inacio">
        <dropAllForeignKeyConstraints baseTableName="ocorrencia"/>
    </changeSet>
    <changeSet id="202204290954-2" author="leonardo.inacio">
        <dropPrimaryKey tableName="status_ocorrencia" constraintName="pk_status_ocorrencia"/>
    </changeSet>
    <changeSet id="202204290954-3" author="leonardo.inacio">
        <renameTable oldTableName="status_ocorrencia" newTableName="status_registro"/>
    </changeSet>
    <changeSet id="202204290954-4" author="leonardo.inacio">
        <addPrimaryKey tableName="status_registro" columnNames="id" constraintName="pk_status_registro"/>
        <addNotNullConstraint tableName="status_registro" columnName="id"/>
    </changeSet>
</databaseChangeLog>
