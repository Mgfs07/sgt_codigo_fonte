<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">

    <changeSet id="202204110901 - 1" author="leonardo.inacio">
        <createTable tableName="prioridade" remarks="Entidade para representar a prioridade de uma ocorrência ">
            <column name="id" type="INT" remarks="Chave primária da tabela prioridade">
                <constraints nullable="false" primaryKey="true" primaryKeyName="pk_prioridade"/>
            </column>
            <column name="descricao" type="VARCHAR(50)" remarks="Descrição da prioridade da ocorrência">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>
